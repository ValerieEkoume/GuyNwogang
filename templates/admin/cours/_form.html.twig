
{{ form_start(form) }}
<div class="row">

    <div class="col-md-4">{{ form_row(form.title) }}</div>
    <div class="col-md-4">{{ form_row(form.description) }}</div>
    <div class="col-md-4">{{ form_row(form.genre) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.niveau) }}</div>
    <div class="col-md-4">{{ form_row(form.prix) }}</div>
    <div class="col-md-4">{{ form_row(form.parts) }}</div>

</div>
<div class="row">


    <div class="col-md-3">{{ form_row(form.free) }}</div>
</div>



{{ form_widget(form) }}
<button class="btn btn-primary">{{ button|default('Eregistrer') }}</button>
{{ form_end(form) }}

